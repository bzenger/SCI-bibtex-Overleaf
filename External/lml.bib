@Article{LML:Aug2003,
  author =       "G. J. Augustine and F. Santamaria and K. Tanaka",
  title =        "Local calcium signaling in neurons",
  journal =      "Neuron",
  volume =       "40",
  number =       "2",
  pages =        "331--46",
  note =         "0896-6273 Journal Article Review Review, Tutorial",
  abstract =     "Transient rises in the cytoplasmic concentration of
                 calcium ions serve as second messenger signals that
                 control many neuronal functions. Selective triggering
                 of these functions is achieved through spatial
                 localization of calcium signals. Several qualitatively
                 different forms of local calcium signaling can be
                 distinguished by the location of open calcium channels
                 as well as by the distance between these channels and
                 the calcium binding proteins that serve as the
                 molecular targets of calcium action. Local calcium
                 signaling is especially prominent at presynaptic active
                 zones and postsynaptic densities, structures that are
                 distinguished by highly organized macromolecular arrays
                 that yield precise spatial arrangements of calcium
                 signaling proteins. Similar forms of local calcium
                 signaling may be employed throughout the nervous
                 system, though much remains to be learned about the
                 molecular underpinnings of these events.",
  keywords =     "Animals Calcium Signaling/*physiology Human
                 Neurons/*physiology Support, Non-U.S. Gov't Support,
                 U.S. Gov't, P.H.S. Synapses/physiology",
  year =         "2003",
}

@Article{LML:Aug89,
  author =       "S. Augst and D. Strickland and D. D. Meyerhofer and S.
                 L. Chin and J. H. Eberly",
  title =        "Tunneling ionization of noble gases in a high
                 intensity laser field",
  journal =      "Physical Review Letters",
  volume =       "63",
  number =       "20",
  pages =        "2212--2215",
  abstract =     "Studies of multiphoton ionization of noble gases have
                 been carried out using 1-µm, 1-ps laser pulses with
                 intensities up to the mid 1016 W/cm2. To our knowledge,
                 this work represents the first study of the production
                 of highly ionized noble-gas ions done exclusively in
                 the tunneling regime. It is found in this regime that
                 the ionization at a given intensity depends on both the
                 ionization potential and the charge state of the
                 species. The onset of ionization occurs when the sum of
                 the Coulomb and laser electric potentials causes the
                 electron to be unbound.",
  year =         "1989",
}

@Article{LML:Aug91,
  author =       "S. Augst and D. D. Meyerhofer and D. Strickland and S.
                 L. Chin",
  title =        "Laser ionization of noble gases by Coulomb-barrier
                 suppression",
  journal =      "Journal of the Optical Society of America B",
  volume =       "8",
  number =       "4",
  pages =        "858--867",
  note =         "Ugh! The none of the theories match the data. They do
                 not include variation of detection efficiency(or
                 signal) with charge state.",
  abstract =     "Laser ionization of noble gases was studied with a
                 1.053-µm, 1-psec Nd:glass laser. A systematic scan of
                 intensities from the mid-1013 W/cm2 to the mid-1016
                 W/cm2 was performed, resulting in the production of
                 charge states as high as Xe12+. Ionization occurs
                 exclusively in the tunneling regime. We compare
                 experimental ion production rates with those predicted
                 by several different theories. Agreement between
                 experimental ion-production curves and theoretical
                 predictions is good for two theoretical models: (1) an
                 elaboration of the Keldysh tunneling model, developed
                 by Ammosov et. al. [Sov. Phys. JETP 64, 1191(1986)] and
                 (2) a much more primative model, based on Coulomb
                 -barrier suppression, in which tunneling and other
                 quantum-mechanical effects are ignored completely. The
                 success of the more primative model suggests that a new
                 term, barrier-suppression ionization, rather than
                 tunneling or multiphoton ionization, may be the most
                 appropriate at this wavelength and in this range of
                 intensities.",
  keywords =     "BSI",
  year =         "1991",
}

