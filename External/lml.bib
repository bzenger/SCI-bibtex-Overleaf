@Article{LML:Hag2000,
  author =       "R. E. Hagar and B. E. Ehrlich",
  title =        "Regulation of the type {III} Ins{P}(3) receptor by
                 Ins{P}(3) and {ATP}",
  journal =      "Biophysical Journal",
  volume =       "79",
  number =       "1",
  pages =        "271--278",
  abstract =     "Many hormones and neurotransmitters raise
                 intracellular calcium (Ca(2+)) by generating InsP(3)
                 and activating the inositol 1,4, 5-trisphosphate
                 receptor (InsP(3)R). Multiple isoforms with distinct
                 InsP(3) binding properties () have been identified ().
                 The type III InsP(3)R lacks Ca(2+)-dependent
                 inhibition, a property that makes it ideal for signal
                 initiation (). Regulation of the type III InsP(3)R by
                 InsP(3) and ATP was explored in detail using planar
                 lipid bilayers. In comparison to the type I InsP(3)R,
                 the type III InsP(3)R required a higher concentration
                 of InsP(3) to reach maximal channel activity (EC(50) of
                 3.2 microM versus 0.5 microM for the types III and I
                 InsP(3)R, respectively). However, the type III InsP(3)R
                 did reach a 2.5-fold higher level of activity. Although
                 activation by InsP(3) was isoform-specific, regulation
                 by ATP was similar for both isoforms. In the presence
                 of 2 microM InsP(3), low ATP concentrations (<6 mM)
                 increased the open probability and mean open time. High
                 ATP concentrations (>6 mM) decreased channel activity.
                 These results illustrate the complex nature of type III
                 InsP(3)R regulation. Enhanced channel activity in the
                 presence of high InsP(3) may be important during
                 periods of prolonged stimulation, whereas allosteric
                 modulation by ATP may help to modulate intracellular
                 Ca(2+) signaling.",
  keywords =     "CALCIUM; CHANNEL; ELECTROPHYSIOLOGY; ENDOPLASMIC
                 RETICULUM; IP3; MODEL",
  year =         "2000",
}

