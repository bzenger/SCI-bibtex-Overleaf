@Article{LML:Hau2002,
  author =       "Jason M. Haugh",
  title =        "A Unified Model for Signal Transduction Reactions in
                 Cellular Membranes",
  journal =      "Biophys. J.",
  volume =       "82",
  number =       "2",
  pages =        "591--604",
  abstract =     "An analytical solution is obtained for the
                 steady-state reaction rate of an intracellular enzyme,
                 recruited to the plasma membrane by active receptors,
                 acting upon a membrane-associated substrate. Influenced
                 by physical and chemical effects, such interactions are
                 encountered in numerous signal-transduction pathways.
                 The generalized modeling framework is the first to
                 combine reaction and diffusion limitations in enzyme
                 action, the finite mean lifetime of receptor-enzyme
                 complexes, reactions in the bulk membrane, and
                 constitutive and receptor-mediated substrate insertion.
                 The theory is compared with other analytical and
                 numerical approaches, and it is used to model two
                 different signaling pathway types. For two-state
                 mechanisms, such as activation of the Ras GTPase, the
                 diffusion-limited activation rate constant increases
                 with enhanced substrate inactivation, dissociation of
                 receptor-enzyme complexes, or crowding of neighboring
                 complexes. The latter effect is only significant when
                 nearly all of the substrate is in the activated state.
                 For regulated supply and turnover pathways, such as
                 phospholipase C-mediated lipid hydrolysis, an
                 additional influence is receptor-mediated substrate
                 delivery. When substrate consumption is rapid, this
                 process significantly enhances the effective enzymatic
                 rate constant, regardless of whether enzyme action is
                 diffusion limited. Under these conditions, however,
                 enhanced substrate delivery can result in a decrease in
                 the average substrate concentration.",
  keywords =     "\#model; SIMULATION; SIGNAL TRANSDUCTION; ip3; plc;
                 membranes;",
  year =         "2002",
}

@Article{LML:Hau75,
  author =       "Hermann A. Haus",
  title =        "Theory of Modelocking with a Slow Saturable Absorber",
  journal =      "IEEE Journal of Quantum Electronics",
  volume =       "QE-11",
  number =       "9",
  pages =        "736--746",
  abstract =     "A closed form solution is presented for the pulses of
                 a homogeneously broadened laser mode locked by a
                 saturable absorber of relaxation time much longer than
                 the pulsewidth. With suitable approximations, the pulse
                 shape is a secant hyperbolic. The system is describe in
                 terms of the linewidth Wc of the system, the saturation
                 energies of the saturable absorber and the laser
                 medium, the ration of the saturable-absorber loss to
                 resonator loss, and the ration of cavity roundtrip time
                 and laser-medium relaxation time. The energy and width
                 of the pulse are obtained as functions of these
                 parameters. Graphs are presented for the operating
