@Article{LML:Rob2001,
  author =       "V. Robert and P. Gurlini and V. Tosello and T. Nagai
                 and A. Miyawaki and F. Di Lisa and T. Pozzan",
  title =        "Beat-to-beat oscillations of mitochondrial [Ca2+] in
