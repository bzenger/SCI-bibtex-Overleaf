@Article{LML:Tin2001,
  author =       "A. Y. Ting and K. H. Kain and R. L. Klemke and R. Y.
                 Tsien",
  title =        "Genetically encoded fluorescent reporters of protein
                 tyrosine kinase activities in living cells",
  journal =      "Proc Natl Acad Sci U S A",
  volume =       "98",
  number =       "26",
  pages =        "15003--8",
  note =         "0027-8424 Journal Article",
  abstract =     "The complexity and specificity of many forms of signal
                 transduction are widely believed to require spatial
                 compartmentation of protein kinase and phosphatase
                 activities, yet existing methods for measuring kinase
                 activities in cells lack generality or spatial or
                 temporal resolution. We present three genetically
                 encoded fluorescent reporters for the tyrosine kinases
                 Src, Abl, and epidermal growth factor (EGF) receptor.
                 The reporters consist of fusions of cyan fluorescent
                 protein (CFP), a phosphotyrosine binding domain, a
                 consensus substrate for the relevant kinase, and yellow
                 fluorescent protein (YFP). Stimulation of kinase
                 activities in living cells with addition of growth
                 factors causes 20-35\% changes in the ratios of yellow
                 to cyan emissions because of phosphorylation-induced
                 changes in fluorescence resonance energy transfer
                 (FRET). Platelet-derived growth factor (PDGF)
                 stimulated Abl activity most strongly in actin-rich
                 membrane ruffles, supporting the importance of this
                 tyrosine kinase in the regulation of cell morphology.
                 These results establish a general strategy for
                 nondestructively imaging dynamic protein tyrosine
                 kinase activities with high spatial and temporal
                 resolution in single living cells.",
  keywords =     "GFP; SIGNAL TRANSDUCTION; MICROSCOPY; IMAGING;
                 FLUORESCENCE;",
  year =         "2001",
}

