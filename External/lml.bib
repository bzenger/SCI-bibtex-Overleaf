@Article{LML:Pre97,
  author =       "J. C. Prechtl and L. B. Cohen and B. Pesaran and P. P.
                 Mitra and D. Kleinfeld",
  title =        "Visual stimuli induce waves of electrical activity in
                 turtle cortex",
  journal =      "Proceedings of the National Academy of Sciences of the
                 United States of America",
  volume =       "94",
  number =       "14",
  pages =        "7621--7626",
  abstract =     "The computations involved in the processing of a
                 visual scene invariably involve the interactions among
                 neurons throughout all of visual cortex. One hypothesis
                 is that the timing of neuronal activity, as well as the
                 amplitude of activity, provides a means to encode
                 features of objects. The experimental data from studies
                 on cat [Gray, C. M., Konig, P., Engel, A. K. \& Singer,
                 W. (1989) Nature (London) 338, 334-337] support a view
                 in which only synchronous (no phase lags) activity
                 carries information about the visual scene. In
                 contrast, theoretical studies suggest, on the one hand,
                 the utility of multiple phases within a population of
                 neurons as a means to encode independent visual
                 features and, on the other hand, the likely existence
                 of timing differences solely on the basis of network
                 dynamics. Here we use widefield imaging in conjunction
                 with voltage-sensitive dyes to record electrical
                 activity from the virtually intact, unanesthetized
                 turtle brain. Our data consist of single-trial
                 measurements. We analyze our data in the frequency
                 domain to isolate coherent events that lie in different
                 frequency bands. Low frequency oscillations (<5 Hz) are
                 seen in both ongoing activity and activity induced by
                 visual stimuli. These oscillations propagate parallel
                 to the afferent input. Higher frequency activity, with
                 spectral peaks near 10 and 20 Hz, is seen solely in
                 response to stimulation. This activity consists of
                 plane waves and spiral-like waves, as well as more
                 complex patterns. The plane waves have an average phase
                 gradient of approximately pi/2 radians/mm and propagate
                 orthogonally to the low frequency waves. Our results
                 show that large-scale differences in neuronal timing
                 are present and persistent during visual processing.",
  keywords =     "DYES; FLUORESCENCE; IMAGING; MEMBRANE POTENTIAL;
                 MICROSCOPY; POTENTIOMETRIC PROBES. APPLICATIONS",
  year =         "1997",
}

@PhdThesis{LML:Pre98,
  author =       "Chrysanthe Preza",
  title =        "Phase Estimation using Rotational Diversity for
                 Differential Interference Contrast Microscopy",
  school =       "University of Washington, Saint Louis",
  type =         "Doctoral",
  abstract =     "The development of computational methods for Di
                 erential-Interference-Contrast (DIC) microscopy is
                 essential for quantitative interpretation of DIC images
                 which provide information related to the gradient of a
                 specimen's optical-path-length (OPL) distribution along
                 the shear direction of the microscope. The work
                 presented in this dissertation pro- vides: 1, imaging
                 models for DIC microscopy that are essential for phase
                 estimation; and 2, a phase-estimation method which
                 permits the calculation of a specimen's phase function
                 or OPL distribution from DIC images. Two- and
                 three-dimensional imaging models for DIC imaging were
                 derived and tested using phantom specimens viewed with
                 several conventional DIC microscopes. Measured DIC
                 images were compared to model predictions which were
                 generated using theoretical PSFs, computer-generated
                 phantoms, and estimated imaging parameters such as bias
                 and shear. Results show anumber of similarities between
                 the point-aperture model and data suggesting that, for
                 many applications, this model may be adequate for phase
                 estimation. Further evaluation of the general model is
                 needed. A phase- estimation method was developed based
                 on our model and a new idea called rotational
                 diversity, which incorporates multiple DIC images
                 acquired at di erent specimen rotations. The method
                 uses the conjugate-gradient optimization approach to
                 estimate a specimen's phase function from multiple DIC
                 images by minimizing either the least squares or the
                 I-divergence discrepancy measure. Results obtained from
                 testing our method with both simulations and measured
                 DIC images demonstrate that: 1, a signi cant
                 improvement in the quanti cation and the resolution of
                 an estimated phase image is achieved when two DIC
                 images are used compared to the phase estimated from a
                 single DIC image; and 2, in some cases, using more than
                 two DIC images at distinct rotation angles improves the
                 estimated phase. The latter was shown with results
                 obtained from simulations in which deviations from
                 ideal conditions were modeled. Encouraging results were
                 also obtained from applica- tion of the method to DIC
                 images of bovine spermatozoa: the OPL summed over a
                 cell in an image estimated from two or more
                 rotationally-diverse DIC was found to be much more
                 reliable (with variability up to 1.3\%) than that
                 computed from single DIC images (which varies up to
                 20\%).",
  year =         "1998",
}

