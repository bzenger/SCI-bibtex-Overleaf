@Article{LML:Yus2000,
  author =       "R. Yuste and A. Majewska and K. Holthoff",
  title =        "From form to function: calcium compartmentalization in
                 dendritic spines",
  journal =      "Nat. Neurosci.",
  volume =       "3",
  number =       "7",
  pages =        "653--659",
  abstract =     "Dendritic spines compartmentalize calcium, and this
                 could be their main function. We review experimental
                 work on spine calcium dynamics. Calcium influx into
                 spines is mediated by calcium channels and by NMDA and
                 AMPA receptors and is followed by fast diffusional
                 equilibration within the spine head. Calcium decay
                 kinetics are controlled by slower diffusion through the
                 spine neck and by spine calcium pumps. Calcium release
                 occurs in spines, although its role is controversial.
                 Finally, the endogenous calcium buffers in spines
                 remain unknown. Thus, spines are calcium compartments
                 because of their morphologies and local influx and
                 extrusion mechanisms. These studies highlight the
                 richness and heterogeneity of pathways that regulate
                 calcium accumulations in spines and the close
                 relationship between the morphology and function of the
                 spine.",
  keywords =     "CALCIUM; FLUORESCENCE; IMAGING; MICROSCOPY; NEURON;
                 NON-LINEAR OPTICAL EFFECT; SPINE",
  year =         "2000",
}

