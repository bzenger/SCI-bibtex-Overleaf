@Article{LML:Iva2001,
  author =       "P. T. Ivanova and B. A. Cerda and D. M. Horn and J. S.
                 Cohen and F. W. McLafferty and H. A. Brown",
  title =        "Electrospray ionization mass spectrometry analysis of
                 changes in phospholipids in {RBL}-2{H3} mastocytoma
