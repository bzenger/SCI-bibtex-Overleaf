@Article{LML:Tre96,
  author =       "R. Trebino and K.W. DeLong and D.N. Fittinghoff and
                 J.N. Sweetser and M. Krumbuegel and J. Bowie",
  title =        "The musical score, the failure of the fundamental
                 theorem of algebra in two dimensions, and the
                 measurement of ultrashort laser pulses.",
  journal =      "Laser Physics",
  volume =       "6",
  number =       "2",
  pages =        "252--255",
  note =         "(International Symposium on Modern Problems on Laser
                 Physics, Novosibirsk, Russia, 28 Aug.-2 Sept. 1995.)
                 MAIK Nauka/Interperiodica Publishing",
  abstract =     "Occasionally in science a number of seemingly
                 unrelated ideas come together and allow the solution of
                 a technological problem that had previously defied
                 solution. Such a situation occurred in our work in the
                 development of a technique for the measurement of the
                 fastest technological events ever created - ultrashort
                 laser pulses - which can be as short as a few times
                 10/sup -15/s long. This time scale is several orders of
                 magnitude shorter than the fastest electronic
                 measurement devices, and so represents quite a
                 difficult problem. Previous methods achieved only vague
                 and indirect measures of such pulses. For example, the
                 standard method measures only the pulse intensity
                 autocorrelation. This measure gives only rough
                 information regarding the time-dependent intensity,
                 I(t), and it gives no information regarding the phase,
                 psi (t), of the full complex electric field, E(t), of
                 an ultrashort laser pulse. The authors discuss a device
                 that can measure both of these quantities and to do so
                 for a single ultrashort laser pulse.",
  year =         "1996",
}

@Article{LML:Tre97,
  author =       "R. Trebino and K. W. DeLong and D. N. Fittinghoff and
                 J. N. Sweetser and M. A. KrumbŸgel and B. A. Richman
                 and D. J. Kane",
  title =        "Measuring Ultrashort Laser Pulses in the
                 Time-Frequency Domain using Frequency-Resolved Optical
                 Gating",
  journal =      "Rev. Sci. Instrum.",
  volume =       "68",
  number =       "9",
  pages =        "3277--95",
  abstract =     "We summarize the problem of measuring an ultrashort
                 laser pulse and describe in detail a technique that
                 completely characterizes a pulse in time:
                 frequency-resolved optical gating. Emphasis is placed
                 on the choice of experimental beam geometry and the
                 implementation of the iterative phase-retrieval
                 algorithm that together yield an accurate measurement
                 of the pulse time-dependent intensity and phase over a
                 wide range of circumstances. We compare several
                 commonly used beam geometries, displaying sample traces
                 for each and showing where each is appropriate, and we
                 give a detailed description of the pulse-retrieval
                 algorithm for each of these cases.",
  year =         "1997",
}

