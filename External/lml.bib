@Article{LML:Jou98,
  author =       "L. S. Jouaville and F. Ichas and J. P. Mazat",
  title =        "Modulation of cell calcium signals by mitochondria",
  journal =      "Molecular and Cellular Biochemistry",
  volume =       "184",
  number =       "1-2 Pt 2",
  pages =        "371--376",
  abstract =     "It is now clearer and clearer that mitochondria play a
                 role, and perhaps an active role, in cell calcium
                 signalling. The fact that mitochondria can exhibit a
                 Ca2+-induced Ca2+ release (mCICR, Ichas et al. [37])
                 reinforces this concept and makes the mitochondria an
                 essential element in the relay of Ca2+ wave
                 propagation. It must be emphasized that the modulation
                 of cell Ca2+ signals by mitochondria depends upon their
                 energetic status, thus making mitochondria an essential
                 link between energy metabolism and calcium signalling
                 inside the cell.",
  keywords =     "CALCIUM; CHANNEL; MEMBRANE POTENTIAL; MITOCHONDRIA;
