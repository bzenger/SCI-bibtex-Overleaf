@Article{LML:Vis2001,
  author =       "A. Visegrady and Z. Lakos and L. Czimbalek and B.
                 Somogyi",
  title =        "Stimulus-dependent control of inositol
                 1,4,5-trisphosphate-induced ca(2+) oscillation
                 frequency by the endoplasmic reticulum ca(2+)-atpase",
  journal =      "Biophysical Journal",
  volume =       "81",
  number =       "3",
  pages =        "1398--1405",
  abstract =     "In many cell types, receptor stimulation evokes
                 cytosolic calcium oscillations with a frequency that
                 depends on agonist dose. Previous studies demonstrated
                 controversial effects of changing the activity of the
                 endoplasmic reticulum Ca(2+)-ATPase upon the frequency
                 of oscillations. By numerical simulations, we found
                 that the model of De Young and Keizer (), unlike other
                 models, can explain the observed discrepancies,
                 assuming that the different experiments were performed
                 at different stimulus levels. According to model
                 predictions, partial inhibition of internal calcium
                 pumps is expected to increase frequency at low stimulus
                 strength and should have an opposite effect at strong
                 stimuli. Similar results were obtained using an
                 analytical estimation of oscillation period, based on
                 calcium-dependent channel activation and inactivation.
                 In experiments on HeLa cells, 4 nM thapsigargin
                 increased the frequency of calcium oscillations induced
                 by 1 and 2.5 &mgr;M histamine but had no effect on
                 supramaximally stimulated cells. In HEp-2 cells, 2 nM
                 thapsigargin slowed down the rapid, ATP-induced
                 oscillations. Our results suggest that in the
                 investigated cell types, the De Young-Keizer model
                 based on inositol 1,4,5-trisphosphate-dependent
                 calcium-induced calcium release can properly describe
                 intracellular calcium oscillations.",
  keywords =     "CALCIUM; ENDOPLASMIC RETICULUM; FLUORESCENCE; IP3;
                 MODEL; SERCA; SIGNALING; SIMULATION",
  year =         "2001",
}

