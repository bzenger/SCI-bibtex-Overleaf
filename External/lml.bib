@Article{LML:Sle2003b,
  author =       "Boris M. Slepchenko and Mark Terasaki",
  title =        "Cyclin aggregation and robustness of bio-switching",
  journal =      "Mol. Biol. Cell",
  pages =        "E03--04--0248",
  abstract =     "During the cell cycle, Cdc2-cyclin B kinase abruptly
                 becomes active and triggers the entry into
                 mitosis/meiosis. Recently, it was found that inactive
                 Cdc2-cyclin B is present in aggregates in immature
                 starfish oocytes and becomes disaggregated at the time
                 of its activation during maturation. We discuss a
                 possible scenario in which aggregation of Cdc2-cyclin B
                 dramatically enhances robustness of this activation. In
                 this scenario, only inactive Cdc2-cyclin B can form
                 aggregates, and the aggregates are in equilibrium with
                 inactive Cdc2-cyclin B in solution. During maturation,
                 the hormone-triggered inactivation of Myt1 depletes the
                 soluble inactive Cdc2-cyclin B, and the turnover leads
                 to dissolution of the aggregates. This phase change,
                 when coupled with the instability of the signaling
                 network, provides a robust bio-switch.",
  year =         "2003",
}

