@Article{LML:Bul99,
  author =       "A. Bullen and P. Saggau",
  title =        "High-speed, random-access fluorescence microscopy:
                 {II}. Fast quantitative measurements with
                 voltage-sensitive dyes [In Process Citation]",
  journal =      "Biophysical Journal",
  volume =       "76",
  number =       "4",
