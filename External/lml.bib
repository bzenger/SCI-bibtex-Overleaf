@Article{LML:Sch2003,
  author =       "Hartmut Schmidt and Edward B. Brown and Beat Schwaller
                 and Jens Eilers",
  title =        "Diffusional Mobility of Parvalbumin in Spiny Dendrites
                 of Cerebellar Purkinje Neurons Quantified by
                 Fluorescence Recovery after Photobleaching",
  journal =      "Biophys. J.",
  volume =       "84",
  number =       "4",
  pages =        "2599--2608",
  abstract =     "Ca2+-binding proteins (CaBPs) represent key factors
                 for the modulation of cellular Ca2+ dynamics.
