@Article{LML:Mos91,
  author =       "J. Mostowski and J. H. Eberly",
  title =        "Approximate atomic ionization rates in the
                 stabilization regime of the Kramers-Henneberger
                 picture",
  journal =      "Journal of the Optical Society of America B",
  volume =       "8",
  number =       "6",
  pages =        "1212--1217",
  abstract =     "We undertake an approximate low-order calculation of
                 the ionization rate of an atom in a strong-field
                 regime, where stabilization, i.e., suppression of
                 ionization, has been predicted. We proceed in the
                 Kramers-Henneberger(K-H) reference frame and take into
                 account time-depenndent corrections to the static K-H
                 potential. Using results from a short-range model
                 atomic potential, we conclude that three stages of
                 ionization can be distinguished: (1) An abrupt
                 adjustment to laser turn-on occurs and causes some
                 ionization; (2) very rapid dep[letion of a major
                 fraction of the ground state probability ends with
                 stabilization of the remaining fraction; and (3) a
                 residual slower ionization ultimately eliminates
                 stabilization.",
  year =         "1991",
}

