@Article{LML:Col70,
  author =       "Stuart A. Collins",
  title =        "Len-system diffraction integral written in terms of
                 matrix optics",
  journal =      "Journal of the Optical Society of America",
  volume =       "60",
  number =       "9",
  pages =        "1168--1177",
  abstract =     "This paper deals with the propagation of coherent
                 light through lens systems; it relates the diffraction
                 theory to ray optics. A diffraction integral is derived
                 which relates the electromagnetic fields on the input
                 plane of the lens system to those on its output plane.
                 The kernal of the diffraction integral is written in
                 terms of the elements of the ray matrix that describes
                 the complete lens system; that kernal indicates a
                 connection between ray optics and diffraction theory.
                 It also provides a simple method for writing the
                 diffraction integral for a lens system. The results are
                 limited to the paraxial ray approximation, but aplly to
                 symmetric and asymmetric lens systems. In the case of
                 asymmetric systems, i. e., those containing rotated
                 elliptical or cylindrical lenses, the ray matrix
                 formalism is extended so as to use a single fourth
                 order matrix. The diffraction integrals derived are
                 applied to optical spatial filtering, optical-beam
                 waveguides, optical resonators, and holography.",
  keywords =     "Lens design diffraction ray tracing filter
                 holographt",
  year =         "1970",
}

