@Article{LML:Kaf2000,
  author =       "E. J. Kaftan and T. Xu and R. F. Abercrombie and B.
                 Hille",
  title =        "Mitochondria shape hormonally induced cytoplasmic
                 calcium oscillations and modulate exocytosis",
  journal =      "Journal of Biological Chemistry",
  volume =       "275",
  number =       "33",
  pages =        "25465--25470",
  abstract =     "Pituitary gonadotropes transduce hormonal input into
                 cytoplasmic calcium ([Ca(2+)](cyt)) oscillations that
                 drive rhythmic exocytosis of gonadotropins. Using
                 Calcium Green-1 and rhod-2 as optical measures of
                 cytoplasmic and mitochondrial free Ca(2+), we show that
                 mitochondria sequester Ca(2+) and tune the frequency of
                 [Ca(2+)](cyt) oscillations in rat gonadotropes.
                 Mitochondria accumulated Ca(2+) rapidly and in phase
                 with elevations of [Ca(2+)](cyt) after GnRH stimulation
                 or membrane depolarization. Inhibiting mitochondrial
                 Ca(2+) uptake by the protonophore CCCP reduced the
                 frequency of GnRH-induced [Ca(2+)](cyt) oscillations
                 or, occasionally, stopped them. Much of the Ca(2+) that
                 entered mitochondria is bound by intramitochondrial
                 Ca(2+) buffering systems. The mitochondrial Ca(2+)
                 binding ratio may be dynamic because [Ca(2+)](mit)
                 appeared to reach a plateau as mitochondrial Ca(2+)
                 accumulation continued. Entry of Ca(2+) into
                 mitochondria was associated with a small drop in the
                 mitochondrial membrane potential. Ca(2+) was extruded
                 from mitochondria more slowly than it entered, and much
                 of this efflux could be blocked by CGP-37157, a
                 selective inhibitor of mitochondrial Na(+)-Ca(2+)
                 exchange. Plasma membrane capacitance changes in
                 response to depolarizing voltage trains were increased
                 when CCCP was added, showing that mitochondria lower
                 the local [Ca(2+)](cyt) near sites that trigger
                 exocytosis. Thus, we demonstrate a central role for
                 mitochondria in a significant physiological response.",
  keywords =     "CALCIUM; MEMBRANE POTENTIAL; MITOCHONDRIA",
  year =         "2000",
}

