@Article{JDT:Azi2014,
  author =       "S. Aziz and A. R. Leon and M. F. El-Chami",
  title =        "The subcutaneous defibrillator: a review of the
                 literature.",
  journal =      j-ACC,
  year =         "2014",
  month =        apr,
  volume =       "63",
  number =       "15",
  pages =        "1473--1479",
  robnote =      "The recently commercially available subcutaneous
                 implantable cardioverter-defibrillator (S-ICD) uses a
                 completely subcutaneous electrode configuration to
                 treat potentially lethal ventricular tachyarrhythmia.
                 Clinical trials have proven its effectiveness in
                 detecting and treating ventricular fibrillation and
                 tachycardia. The S-ICD offers the advantage of
                 eliminating the need for intravenous and intracardiac
                 leads and their associated risks and shortcomings.
                 However, its major disadvantage is its inability to
                 provide bradycardia rate support and antitachycardia
                 pacing to terminate ventricular tachycardia. This paper
                 discusses the S-ICD clinical trials and advantages and
                 disadvantages of this novel technology to help the
                 physician identify its role and select candidate
                 patients who will benefit from this device.",
  bibdate =      "Fri 19 Sep 2014 12:57:52 PM MDT",
}

